{
  "name": "formwood",
  "version": "1.5.1",
  "main": "dist/formwood.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericvaladas/formwood.git"
  },
  "keywords": [
    "react",
    "forms",
    "form",
    "validation"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run lint && npm run test && webpack",
    "prepare": "npm run clean && npm run build",
    "test": "NODE_ENV=test nyc --reporter=html --reporter=text mocha test/unit",
    "test-coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "require": [
      "babel-core/register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "Eric Valadas <ericvaladas@gmail.com> (https://github.com/ericvaladas)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericvaladas/formwood/issues"
  },
  "homepage": "https://github.com/ericvaladas/formwood",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.1.2",
    "coveralls": "^2.13.1",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.0.0",
    "eslint": "4.7.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.4.0",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "pleaserc": "^2.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.13.0",
    "webpack": "^3.6.0"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "dependencies": {
    "prop-types": "^15.6.0"
  }
}
